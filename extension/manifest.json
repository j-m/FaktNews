{
	"manifest_version": 2,
	"name": "FaktNews",
	"description": "Analises the current webpage and grades its trustworthiness",
	"version": "1.0",
	"browser_action": {
		"default_title": "FaktNews",
		"default_popup": "index.html"
	},
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"permissions": ["tabs", "<all_urls>"]
}