{
	"manifest_version": 3,
	"name": "FaktNews",
	"description": "Analises the current webpage and grades its trustworthiness",
	"version": "2.0",
	"browser_action": {
		"default_title": "FaktNews",
		"default_popup": "index.html"
	},
	"icons": {
		"16": "icon.svg",
		"48": "icon.svg",
		"128": "icon.svg"
	},
	"permissions": ["tabs", "<all_urls>"]
}